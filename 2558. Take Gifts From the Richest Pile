class Solution {
    public long pickGifts(int[] gifts, int k) {
        PriorityQueue<Integer>pq=new PriorityQueue<>(Collections.reverseOrder());
       for(int num : gifts){
        pq.add(num);
       }
       for(int i=0;i<k;i++){
         int largest=pq.poll();
         int r=(int) Math.sqrt(largest);
         pq.add(r);
       }long sum=0;
       for(int num:pq){
        sum+=num;
       }return sum;

    }
}
