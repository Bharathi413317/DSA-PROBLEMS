import java.util.*;
class Main {
    public static int LLlength(Node head){
    Node temp=head;
    int c=0;
    while(temp!=null)
    {
        c++;
        temp=temp.next;
        
    }return c;
    
}
public static int search(Node head,int key){
    Node temp=head;
    int indx=0;
    while(temp!=null){
        if(temp.data == key){
            return indx;
        } 
        indx+=1;  
        temp=temp.next;
    }return -1;
}
    
    public static void printlinkedlist(Node head){
       Node temp=head ;
       
       while(temp!=null){
           if(temp.next==null){
                System.out.print(temp.data);
           }else{
           System.out.print(temp.data+"->");}
           temp=temp.next;
       }System.out.println();
       
       
       
    }


public static void main(String[] args) {
        Node node1=new Node(10);
        Node node2=new Node(89);
         Node node3=new Node(8);
        
        node1.next=node2;
        node2.next=node3;
        node3.next=null;
        
        Node head=node1;
        head.data=90;
        printlinkedlist( head);
        System.out.println(LLlength(head));
        System.out.println(search(head,8));
        
        
}
}
class Node{
    int data;
    Node next;
   public Node (int data)
{       this.data=data;
        this.next=next;
    }
} 
