class Solution {
    public boolean ispossible(int []nums,int n,int k){
         int temp=0;
        for(int i=0;i<nums.length;i++){
            temp=nums[i]/k;
            if(nums[i]%k!=0){
                temp+=1;
            }n-=temp;
            if(n<0){
                return false;
            }

        }return true;
    }
    public int minimizedMaximum(int n, int[] quantities) {
        int l=1;
        int r=(int)Math.pow(10,5);
        while(l<=r){
            int mid=l+(r-l)/2;
            if(ispossible(quantities,n,mid)){
                r=mid-1;
            }else{
                l=mid+1;
            }
        }return l;
           
    }
}
